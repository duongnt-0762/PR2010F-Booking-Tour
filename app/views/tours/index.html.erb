<div>
<div class="header">
  <div class="header1">
    <section>
      <div class="amount-tours">Hơn 300 chỗ ở</div>

      <div class="nearest">Chỗ ở tại khu vực bản đồ đã chọn</div>

    </section>
  </div>
<div class="content-tour">
  <% @tours.each do |tour|%>
  <div class="container">
    <div class="mar-container">
      <a href="#">

        <div class="frame-image">
          <div class="image" >
            <img style='height: 100%; width: 100%; object-fit: contain' src="https://a0.muscache.com/im/pictures/0ec329ab-6ad7-40ea-88e4-4c3e803c0891.jpg?im_w=720" alt="">
          </div>
        </div>
          <div class="hang1">
            <h4><%= tour.title %> <span><%= tour.address %></span></h4>
          <div class="hang2">
            <span><%= tour.description %></span>
            <div class="hang3">
              <span>$<span><%= tour.price %>/đêm</span></span>
            </div>
          </div>
          </div>
      </a>
    </div>
  </div>
  <%end%>
<%= will_paginate %>
</div>
</div>
</div>
